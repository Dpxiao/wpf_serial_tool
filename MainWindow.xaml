    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="MyButtonStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <Style x:Key="DataGridColumnHeaderCenter" TargetType="{x:Type DataGridColumnHeader}">
                <Setter Property="HorizontalAlignment" Value="Center"/>
            </Style>
            <ContextMenu x:Key="MyContextMenu">
                <MenuItem Header="向上插入一行" Click="Add_Up_One_Row_Click"/>
                <MenuItem Header="向下插入一行" Click="Add_Dl_One_Row_Click"/>
                <MenuItem Header="末尾新增一行" Click="Add_End_One_Row_Click"/>
                <MenuItem Header="末尾新增多行" Click="Add_End_Multi_Row_Click"/>
                <MenuItem Header="删除选中行"   Click="Del_Sel_One_Row_Click"/>
                <MenuItem Header="修改当前注释" Click="Modify_Note_Row_Click"/>
            </ContextMenu>
        </ResourceDictionary>

    </Window.Resources>

